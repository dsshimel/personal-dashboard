    },
    {
      "id": 8,
      "title": "WebSocket Reconnections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 28 },
      "targets": [
        {
          "expr": "rate(client_ws_reconnects_total[5m])",
          "legendFormat": "Reconnects/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "fillOpacity": 20, "lineWidth": 2, "drawStyle": "bars" }
        }
      },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" }
    }
  ],
  "schemaVersion": 39
}
