        {
          "expr": "histogram_quantile(0.95, rate(web_vital_fid_seconds_bucket[5m]))",
          "legendFormat": "p95",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "fillOpacity": 10, "lineWidth": 2 }
        }
      },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" }
    },
    {
      "id": 4,
      "title": "Cumulative Layout Shift (CLS)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(web_vital_cls_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.75, rate(web_vital_cls_bucket[5m]))",
