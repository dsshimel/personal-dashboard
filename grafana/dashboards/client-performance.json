        }
      },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" }
    },
    {
      "id": 7,
      "title": "WebSocket Latency",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(client_ws_latency_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.75, rate(client_ws_latency_seconds_bucket[5m]))",
          "legendFormat": "p75",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, rate(client_ws_latency_seconds_bucket[5m]))",
