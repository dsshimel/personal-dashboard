          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": { "fillOpacity": 10, "lineWidth": 2 }
        }
      },
      "datasource": { "type": "prometheus", "uid": "PBFA97CFB590B2093" }
    },
    {
      "id": 5,
      "title": "Interaction to Next Paint (INP)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(web_vital_inp_seconds_bucket[5m]))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.75, rate(web_vital_inp_seconds_bucket[5m]))",
          "legendFormat": "p75",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.95, rate(web_vital_inp_seconds_bucket[5m]))",
